#include <stdio.h>
#include <stdlib.h>



    int main() {

    int num1, num2, num3;


    printf("Digite o primeiro numero: ");
    scanf("%d", &num1);

    printf("Digite o segundo numero: ");
    scanf("%d", &num2);

    printf("Digite o terceiro numero: ");
    scanf("%d", &num3);


    printf("Os numeros na ordem inversa sao: %d, %d, %d\n", num3, num2, num1);

    return 0;
}
